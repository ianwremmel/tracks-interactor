{
  "name": "@ianwremmel/tracks-interactor",
  "version": "0.0.0-development",
  "description": "",
  "bin": {},
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ianwremmel/tracks-interactor.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:readme": "markdown-toc -i --bullets='-' --maxdepth=3  README.md  && prettier --write README.md",
    "build:es": "babel --source-maps --ignore '**/*.d.ts' --extensions '.js,.ts' -d dist src",
    "build:types": "tsc --emitDeclarationOnly",
    "eslint": "eslint --ignore --ignore-path .gitignore --ext .js --ext .ts",
    "lint": "npm-run-all lint:*",
    "lint:changelog": "commitlint --from origin/master --to HEAD",
    "lint:js": "npm run --silent eslint -- .",
    "lint:types": "tsc --noEmit",
    "semantic-release": "semantic-release",
    "test": "jest"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [],
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "@ianwremmel/exception": "^3.0.0",
    "immer": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@ianwremmel/eslint-plugin-ianwremmel": "^3.0.0",
    "@types/chai": "^4.1.7",
    "@types/jest": "^24.0.11",
    "@types/node": "^13.1.0",
    "@typescript-eslint/parser": "^2.0.0",
    "chai": "^4.2.0",
    "condition-circle": "^2.0.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^23.0.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.0",
    "jest": "^24.7.1",
    "jest-junit": "^10.0.0",
    "lint-staged": "^9.0.2",
    "markdown-toc": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "semantic-release": "^15.13.3",
    "typescript": "^3.4.2"
  },
  "lint-staged": {
    "*.js": "npm run eslint -- ",
    "*.ts": "npm run eslint -- ",
    "bin/**": "npm run eslint -- "
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "publishConfig": {
    "access": "public"
  }
}
